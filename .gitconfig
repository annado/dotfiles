[user]
	name = Anna Do
	email = anna@aplusd.co
[core]
	editor = vi
	excludesfile = ~/src/dotfiles/.gitignore
[credential]
	helper = osxkeychain
[alias]
	st = status
	co = checkout
  br = branch
  undo = reset --soft HEAD~
  patch = add --patch
  merged = branch --merged
  prune = git remote prune origin
[pull]
	rebase = true
[color]
	ui = auto
[color "branch"]
  current = cyan reverse
  local = cyan
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red
  new = green
[color "status"]
  added = green
  changed = red
  untracked = cyan
[push]
  default = simple
